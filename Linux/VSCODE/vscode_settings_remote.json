{
    "vs-kubernetes": {
        "vs-kubernetes.minikube-path.linux": "/home/shukla20/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
        "vs-kubernetes.helm-path.linux": "/home/shukla20/.vs-kubernetes/tools/helm/linux-amd64/helm",
        "vs-kubernetes.draft-path.linux": "/home/shukla20/.vs-kubernetes/tools/draft/linux-amd64/draft"
    },
    "remote.SSH.remotePlatform": {
        "<hostname>": "linux"
    },
    "files.associations": {
        "*.yaml": "ansible",
        "*.yml": "ansible"
    },
    "gitlens.remotes": [
        {
            "domain": "abcd.com",
            "type": "GitHub"
        }
    ],
    "editor.rulers": [
        79,
        99,
        119
    ],
    "[ansible]": {
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.tabSize": 2
    },
    "vim.foldfix": true,
    "vim.smartcase": false,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "u"
            ],
            "after": [],
            "commands": [
                {
                    "command": "undo",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "after": [],
            "commands": [
                {
                    "command": "redo",
                    "args": []
                }
            ]
        }
    ],
    "terminal.integrated.shell.linux": "/usr/bin/zsh",
    "gitlens.gitCommands.skipConfirmations": [
        "fetch:command",
        "stash-push:command",
        "switch:command",
        "switch:menu"
    ],
    "workbench.colorTheme": "Atom One Dark",
    "editor.fontLigatures": true,
    "editor.renderWhitespace": "all",
    "terminal.integrated.copyOnSelection": true,
    "indentRainbow.colors": [

        "rgba(255,255,64,0.07)",
        "rgba(127,255,127,0.07)",
        "rgba(255,127,255,0.07)",
        "rgba(79,236,236,0.07)"
    ],
    "workbench.iconTheme": "material-icon-theme",
    "templateFinder.extension.activated": true,
    "cSpell.enableFiletypes": [
        "ansible"
    ],
    "cSpell.enabled": true,
    "editor.minimap.enabled": false,
    "gitlens.advanced.telemetry.enabled": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "vim.cursorStylePerMode.insert": "line-thin",
    "git.autoStash": true,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "php.suggest.basic": false,
    "remote.autoForwardPortsSource": "output",
    "swaggerViewer.defaultHost": "",
	"http.proxyStrictSSL": false,
	"files.insertFinalNewline": true,
	"editor.fontFamily": "Input Mono"
}
